
public class Conta {

	private double saldo;
	private String nome;

	
	public Conta(double saldo, String nome){
		this.saldo = saldo;
		this.nome = nome;
	}
	
	public double getSaldo(){
		return saldo;
	}
	
	public void setSaldo(double saldo){
		this.saldo = saldo;
	}
	
	
	
	
}
