
public class Arrojado implements Investimento{

	@Override
	public double calcula(Conta conta) {
		boolean pior = new java.util.Random().nextDouble() > 0.50;
		if(pior)
			return conta.getSaldo() * 0.006;
		else 
			boolean melhor = new java.util.Random().nextDouble() > 0.30;
		}
	}

}
